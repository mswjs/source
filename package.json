{
  "name": "@mswjs/source",
  "version": "0.0.0",
  "description": "Generate request handlers for Mock Service Worker from various sources.",
  "main": "lib/umd/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:mswjs/source.git",
  "author": "Artem Zakharchenko <kettanaito@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint ./{src,test}/**/*.ts",
    "har:fixture": "ts-node test/traffic/fixtures/requests/command.ts",
    "clean": "rimraf ./lib",
    "build": "yarn lint && yarn clean && rollup -c rollup.config.ts",
    "test": "jest test",
    "prepublishOnly": "yarn build && yarn test:lib && yarn test"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">= 12.18.0"
  },
  "peerDependencies": {
    "msw": "^0.26.2"
  },
  "devDependencies": {
    "@open-draft/test-server": "^0.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/compression": "^1.7.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "compression": "^1.7.4",
    "cross-fetch": "^3.0.6",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "msw": "^0.34.0",
    "playwright": "^1.13.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@types/har-format": "^1.2.7",
    "headers-utils": "^3.0.2",
    "openapi-types": "^7.2.3",
    "outvariant": "^1.2.0"
  }
