{
  "name": "@mswjs/source",
  "version": "0.0.0",
  "description": "Generate request handlers for Mock Service Worker from various sources.",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    }
  },
  "repository": "git@github.com:mswjs/source.git",
  "author": "Artem Zakharchenko <kettanaito@gmail.com>",
  "scripts": {
    "start": "pnpm build --watch",
    "lint": "eslint ./{src,test}/**/*.ts",
    "har:fixture": "ts-node test/traffic/fixtures/requests/command.ts",
    "prepare": "pnpm simple-git-hooks init",
    "prebuild": "pnpm lint",
    "build": "tsup src/index.ts --format cjs,esm --out-dir lib --dts --clean",
    "test": "jest",
    "prepublishOnly": "pnpm test && pnpm build"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "msw": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@open-draft/test-server": "^0.2.3",
    "@types/compression": "^1.7.1",
    "@types/jest": "^26.0.20",
    "@types/node": "18",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "compression": "^1.7.4",
    "cross-fetch": "^3.1.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "msw": "^1.2.1",
    "playwright": "^1.13.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "ts-jest": "26.5.6",
    "ts-node": "^10.2.0",
    "tslib": "^2.3.1",
    "tsup": "^5.11.13",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@types/faker": "^5.5.9",
    "@types/har-format": "^1.2.7",
    "@types/set-cookie-parser": "^2.4.2",
    "faker": "5.x",
    "headers-utils": "^3.0.2",
    "openapi-types": "^7.2.3",
    "outvariant": "^1.2.1",
    "randexp": "^0.5.3",
    "set-cookie-parser": "^2.6.0"
  }
}
