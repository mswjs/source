{
  "type": "module",
  "name": "@mswjs/source",
  "version": "0.0.0",
  "description": "Generate MSW request handlers from OpenAPI documents, HAR archives, and other sources.",
  "exports": {
    "./open-api": {
      "types": "./lib/open-api/from-open-api.d.ts",
      "default": "./lib/open-api/from-open-api.js"
    },
    "./traffic": {
      "types": "./lib/traffic/from-traffic.d.ts",
      "default": "./lib/traffic/from-traffic.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "repository": "git@github.com:mswjs/source.git",
  "author": "Artem Zakharchenko <me@kettanaito.com>",
  "scripts": {
    "start": "pnpm build --watch",
    "har:fixture": "ts-node ./test/traffic/fixtures/requests/command.ts",
    "prepare": "pnpm simple-git-hooks init",
    "prebuild": "pnpm lint",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "vitest",
    "prepack": "pnpm test && pnpm build"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  },
  "engines": {
    "node": ">=20.11"
  },
  "packageManager": "pnpm@8.15.6",
  "peerDependencies": {
    "msw": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@open-draft/test-server": "^0.5.1",
    "@types/compression": "^1.7.1",
    "@types/node": "18",
    "compression": "^1.7.4",
    "jsdom": "^22.1.0",
    "lint-staged": "^10.5.3",
    "msw": "^2.3.0",
    "playwright": "^1.34.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.8.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@types/faker": "^5.5.9",
    "@types/har-format": "^1.2.7",
    "faker": "5.x",
    "openapi-types": "^7.2.3",
    "outvariant": "^1.2.1",
    "randexp": "^0.5.3"
  }
}
